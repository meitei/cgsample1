  <form id="kokyakuSearchForm">
  <div>
    <table>
      <tr>
        <th style="width:80px;">顧客コード</th>
        <td style="width:210px;">
          <input style="width:60px;" type="text" id="kokyakuIdFrom" name="kokyaku[kokyakuIdFrom]" tabindex="2" />&nbsp;〜
          <input style="width:60px;" type="text" id="kokyakuIdTo" name="kokyaku[kokyakuIdTo]" tabindex="4" />
        </td>
        <th style="width:80px;">郵便番号</th>
        <td style="width:175px;">
          <input style="width:130px;" type="text" id="postNo" name="kokyaku[postNo]" tabindex="28" />
        </td>
        <th>FAX番号</th>
        <td>
          <input style="width:130px;" type="text" id="fax" name="kokyaku[fax]" tabindex="38" />
        </td>
      </tr>
      <tr>
        <th>顧客名</th>
        <td>
          <input style="width:130px;" type="text" id="kokyakuNm" name="kokyaku[kokyakuNm]" tabindex="6" />
        </td>
        <th>住所１</th>
        <td>
          <input style="width:130px;" type="text" id="address1" name="kokyaku[address1]" tabindex="30" />
        </td>
        <th>傷病名</th>
        <td>
          <input style="width:130px;" type="text" id="shobyouNm" name="kokyaku[shobyoNm]" tabindex="40" />
        </td>
      </tr>
      <tr>
        <th>顧客カナ</th>
        <td>
          <input style="width:130px;" type="text" id="kokyakuNmKana" name="kokyaku[kokyakuNmKana]" tabindex="8" />
        </td>
        <th>住所２</th>
        <td>
          <input style="width:130px;" type="text" id="address2" name="kokyaku[address2]" tabindex="32" />
        </td>
        <th>学校名</th>
        <td>
          <input style="width:130px;" type="text" id="gakkoNm" name="kokyaku[gakkoNm]" tabindex="42" />
        </td>
      </tr>
      <tr>
        <th>性別</th>
        <td>
          <%= select :kokyaku, :seibetsu, [["男性", "0"], ["女性", "1"]], :include_blank => true %>
        </td>
        <th>電話番号</th>
        <td>
          <input style="width:130px;" type="text" id="tel1" name="kokyaku[tel1]" tabindex="34" />
        </td>
      </tr>
      <tr>
        <th>生年月日</th>
        <td>　
          <%= select :kokyaku, :tanjoGengoFrom, [["大正", "1"], ["昭和", "2"], ["平成", "3"]], :include_blank => true %>
          <input style="width:20px; ime-mode: disabled;" type="text" id="tanjoYearFrom" name="kokyaku[tanjoYearFrom]" tabindex="99" />年
          <input style="width:20px; ime-mode: disabled;" type="text" id="tanjoMonthFrom" name="kokyaku[tanjoMonthFrom]" tabindex="99" />月
          <input style="width:20px; ime-mode: disabled;" type="text" id="tanjoDayFrom" name="kokyaku[tanjoDayFrom]" tabindex="99" />日
          <br />〜
          <%= select :kokyaku, :tanjoGengoTo, [["大正", "1"], ["昭和", "2"], ["平成", "3"]], :include_blank => true %>
          <input style="width:20px;" type="text" id="tanjoYearTo" name="kokyaku[tanjoYearTo]" tabindex="99" />年
          <input style="width:20px;" type="text" id="tanjoMonthTo" name="kokyaku[tanjoMonthTo]" tabindex="99" />月
          <input style="width:20px;" type="text" id="tanjoDayTo" name="kokyaku[tanjoDayTo]" tabindex="99" />日
        </td>
        <th style="width:80px;">携帯番号</th>
        <td style="width:175px;">
          <input style="width:130px;" type="text" id="tel2" name="kokyaku[tel2]" tabindex="36" />
        </td>
      </tr>
    </table>
    <div class="btn_submit">
      <div><a class="btn_search" id="btnCustomerSearch" href="#" tabindex="44" />検索</a></div>
      <div><a class="btn_clear" id="btnCustomerClear" href="#" tabindex="46" />条件クリア</a></div>
    </div>
  </div>
  <div>
  </div>
  </form>
<script type="text/javascript">
  $("#kokyaku_tanjoGengoFrom").css("width", "60px").attr("tabindex", "99"); //TODO:tabindex
  $("#kokyaku_tanjoGengoTo").css("width", "60px").attr("tabindex", "99"); //TODO:tabindex
</script>
