    <table>
      <tr>
        <th style="width:80px;">顧客コード</th>
        <td style="width:175px;">
          <input style="width:50px; ime-mode: disabled;" type="text" id="kokyakuIdFrom" name="konyu_rireki[kokyakuIdFrom]" maxlength="7" />&nbsp;〜
          <input style="width:50px; ime-mode: disabled;" type="text" id="kokyakuIdTo" name="konyu_rireki[kokyakuIdTo]" maxlength="7" />
        </td>
        <th style="width:70px;">保険種別１</th>
        <td style="width:175px;">
          <%= select :konyu_rireki, :hokenShubetsuCd1, @hoken_shubetsu.map{|t| [t.hokenShubetsuNm, t.hokenShubetsuCd]}, :include_blank => true %>
        </td>
        <th style="width:60px;">受注日</th>
