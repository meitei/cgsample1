          <input style="width:80px; ime-mode: disabled;" type="text" id="mitsumoriDtTo" name="konyu_rireki[mitsumoriDtTo]" />
        </td>
      </tr>

    </table>
    <div class="btn_submit">
      <div><a class="btn_search" id="btnKonyuRirekiSearch" href="#" />検索</a></div>
      <div><a class="btn_clear" id="btnKonyuRirekiClear" href="#" />条件クリア</a></div>
    </div>
  </div>
  <div>
  </div>
  </form>
<script type="text/javascript">
  $("#konyuRirekiSearchForm td").css("text-align", "left");

  // tabindex一括設定
  var items = new Array(
    "#kokyakuIdFrom",
    "#kokyakuIdTo",
    "#byoinNm",
    "#kokyakuNm1",
    "#kokyakuNm2",
    "#kokyakuNmKana1",
    "#kokyakuNmKana2",
    "#konyu_rireki_shubetsuKn",
    "#hinmeiNm",
    "#konyu_rireki_hokenShubetsuCd1",
    "#konyu_rireki_hokenShubetsuCd2",
    "#shohinNm",
    "#uketsukeSesakuTantoNm",
    "#kariAwaseTantoNm",
    "#nohinTantoNm",
    "#mitsumoriTantoEigyoNm",
    "#juchuDtFrom",
    "#juchuDtTo",
    "#kariAwaseDtFrom",
    "#kariAwaseDtTo",
    "#nohinDtFrom",
    "#nohinDtTo",
    "#kofuDtFrom",
    "#kofuDtTo",
    "#nyukinDtFrom",
    "#nyukinDtTo",
    "#oshiinDtFrom",
    "#oshiinDtTo",
    "#kanryoDtFrom",
    "#kanryoDtTo",
    "#mitsumoriDtFrom",
    "#mitsumoriDtTo",
    "#btnKonyuRirekiSearch",
    "#btnKonyuRirekiClear",
    "#btnKonyuRirekiNew"
  );
  for(i=0 ; i<items.length; i++) {
    $(items[i]).attr("tabindex", i+1);
  }
  $(items[0]).select();
</script>