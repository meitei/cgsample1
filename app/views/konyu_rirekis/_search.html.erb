      </tr>
      <tr>
        <th>病院</th>
        <td>
          <input style="width:130px;" type="text" id="byoinNm" name="konyu_rireki[byoinNm]" />
        </td>
        <th>保険種別２</th>
        <td>
          <%= select :konyu_rireki, :hokenShubetsuCd2, @hoken_shubetsu.map{|t| [t.hokenShubetsuNm, t.hokenShubetsuCd]}, :include_blank => true %>
        </td>
        <th>仮合日</th>
        <td>
          <input style="width:80px; ime-mode: disabled;" type="text" id="kariAwaseDtFrom" name="konyu_rireki[kariAwaseDtFrom]" />&nbsp;〜
          <input style="width:80px; ime-mode: disabled;" type="text" id="kariAwaseDtTo" name="konyu_rireki[kariAwaseDtTo]" />
        </td>
      </tr>
      <tr>
        <th>顧客名</th>
        <td>
          <input style="width:65px;" type="text" id="kokyakuNm1" name="konyu_rireki[kokyakuNm1]" />
          <input style="width:65px;" type="text" id="kokyakuNm2" name="konyu_rireki[kokyakuNm2]" />
