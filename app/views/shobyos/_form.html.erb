<div id="shobyoFormDialog" title="傷病名登録画面" class="Tourokuform cf" style="overflow:hidden;display: none;">
  <form id="shobyoForm">
  <div>
    <table>
      <tr>
        <th style="width:80px;">傷病名称</th>
        <td style="width:175px;">
          <input style="width:130px;" type="text" id="shobyoNm" name="shobyoNm" />
        </td>
        <th style="width:80px;">傷病名称カナ</th>
        <td style="width:175px;">
          <input style="width:130px;" type="text" id="shobyoNmKana" name="shobyoNmKana" />
        </td>
      </tr>
      <tr>
        <th style="width:80px;">収載日</th>
        <td style="width:175px;">
          <input style="width:130px;" type="text" id="shusaiDt" name="shusaiDt" />
        </td>
        <th style="width:80px;">廃止日</th>
        <td style="width:175px;">
          <input style="width:130px;" type="text" id="haishiDt" name="haishiDt" />
        </td>
      <tr>
        <th style="width:80px;">ICD10コード</th>
        <td style="width:175px;">
          <input style="width:130px;" type="text" id="icd10Cd" name="icd10Cd" />
        </td>
      </tr>
    </table>
  </div>
  </form>
<script>
$(function() {
  $("#shusaiDt").datepicker();
  $("#haishiDt").datepicker();

  $("#shobyoForm").validate({
        errorClass:'error',
        errorElement:'label',
        highlight: function (element, errorClass, validClass) { 
            $(element).addClass(errorClass); 
        }, 
        unhighlight: function (element, errorClass, validClass) { 
            $(element).removeClass(errorClass); 
        },
        rules: {
            shobyoNm: {required:true, maxlength:25},
            shobyoNmKana: {required:true, katakana:true, maxlength:25},
            shusaiDt: {date:true},
            haishiDt: {date:true}
        }
    });

});
</script>
